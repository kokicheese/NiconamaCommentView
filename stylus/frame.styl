
c_1 = #EEE

btn()
  background c_1
  border-width 3px
  color #111
  padding 10px 0

line-top(op1=.2, op2=.3)
  border-top: 1px solid rgba 0, 0, 0, op1
  box-shadow rgba(255, 255, 255, op2) 0 1px 0 inset

line-right(op1=2, op2=3)
  border-right 1px solid rgba(0, 0, 0, op1)
  box-shadow rgba(255, 255, 255, op2) -1px 0 0 inset

line-buttom()
  border-buttom
  box-shadow rgba(255, 255, 255, op2)0 1px 0

line-left()
  border-left
  box-shadow rgba(255, 255, 255, op2) -1px 0 0

wh100()
  height 100%
  width 100%

html, body
  margin 0
  padding 0
  wh100()

#windowFrame 
  //background c_1 url('images/background.png')
  wh100()
  position absolute
  top 0
  left 0

#main
  wh100()
  position absolute
  top 0
  left 0

#header
  width 100%
  height 15.2%
  background url('images/background.png')